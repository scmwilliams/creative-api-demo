<h1>Listing asset_groups</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asset_groups.each do |asset_group| %>
      <tr>
        <td><%= asset_group.brand %></td>
        <td><%= asset_group.name %></td>
        <td><%= link_to 'Show', asset_group %></td>
        <td><%= link_to 'Edit', edit_asset_group_path(asset_group) %></td>
        <td><%= link_to 'Destroy', asset_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asset group', new_asset_group_path %>
